{
	"kind": "collectionType",
	"collectionName": "wallpapers",
	"info": {
		"singularName": "wallpaper",
		"pluralName": "wallpapers",
		"displayName": "Wallpaper",
		"description": ""
	},
	"options": {
		"draftAndPublish": true
	},
	"pluginOptions": {},
	"attributes": {
		"client": {
			"type": "enumeration",
			"enum": ["pc"]
		},
		"color": {
			"type": "customField",
			"regex": "^#([A-Fa-f0-9]{6}|[A-Fa-f0-9]{3})$",
			"customField": "plugin::color-picker.color"
		},
		"photo": {
			"type": "media",
			"multiple": false,
			"required": true,
			"allowedTypes": ["images", "files", "videos", "audios"]
		},
		"tag": {
			"type": "relation",
			"relation": "oneToOne",
			"target": "api::wallpaper-tag.wallpaper-tag"
		}
	}
}
