{
  "name": "wxt-react-starter",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "description": "manifest.json description",
  "scripts": {
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
    "prebuild": "pnpm run --filter @onetab/web-app build:lib",
    "build": "wxt build",
    "build:firefox": "wxt build -b firefox",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip -b firefox",
    "compile": "tsc --noEmit",
    "postinstall": "wxt prepare"
  },
  "dependencies": {
    "@onetab/ui": "workspace:*",
    "@onetab/web-app": "workspace:*",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@wxt-dev/module-react": "^1.1.1",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.45",
    "tailwindcss": "^3.4.11",
    "typescript": "^5.6.2",
    "wxt": "^0.19.9"
  }
}
